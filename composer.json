{
    "name": "wyrihaximus/react-cron",
    "description": "Cronlike scheduler running inside a ReactPHP Event Loop",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "dragonmantank/cron-expression": "^3.1",
        "react/async": "^4",
        "react/event-loop": "^1.3",
        "react/promise": "^2.9",
        "thecodingmachine/safe": "^2.4 || ^1.3",
        "wyrihaximus/constants": "^1.6",
        "wyrihaximus/react-mutex": "^3.1 || ^2",
        "wyrihaximus/react-mutex-contracts": "^2.1 || ^1.1"
    },
    "require-dev": {
        "wyrihaximus/async-test-utilities": "^5.0.25"
    },
    "autoload": {
        "psr-4": {
            "WyriHaximus\\React\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WyriHaximus\\Tests\\React\\Cron\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "ergebnis/composer-normalize": true,
            "icanhazstring/composer-unused": true,
            "infection/extension-installer": true
        },
        "platform": {
            "php": "8.1.0"
        },
        "sort-packages": true
    },
    "extra": {
        "unused": [
            "wyrihaximus/react-mutex",
            "wyrihaximus/react-mutex-contracts"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "composer normalize"
        ],
        "post-update-cmd": [
            "composer normalize"
        ]
    }
}
