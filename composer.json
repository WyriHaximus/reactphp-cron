{
    "name": "wyrihaximus/react-cron",
    "description": "Cronlike scheduler running inside a ReactPHP Event Loop",
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "dragonmantank/cron-expression": "^3.1",
        "react/event-loop": "^1.1",
        "react/promise": "^2.7",
        "thecodingmachine/safe": "^1.3",
        "wyrihaximus/constants": "^1.6",
        "wyrihaximus/react-mutex": "^2.0",
        "wyrihaximus/react-mutex-contracts": "^1.0"
    },
    "require-dev": {
        "wyrihaximus/async-test-utilities": "^3.4.17"
    },
    "config": {
        "platform": {
            "php": "7.4.7"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "WyriHaximus\\React\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WyriHaximus\\Tests\\React\\Cron\\": "tests/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "composer normalize"
        ],
        "post-update-cmd": [
            "composer normalize"
        ]
    }
}
